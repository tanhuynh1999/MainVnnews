                                <div class="checkbox">
                                    @Html.EditorFor(model => model.editor_sex)
                                    @Html.ValidationMessageFor(model => model.editor_sex, "", new { @class = "text-danger" })
                                </div>
                            </div>
                        </div>

                        <div class="form-group">
